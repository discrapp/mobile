{
  "expo": {
    "name": "Discr",
    "slug": "discr",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "com.discr.app",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#3b1877"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.discr.app",
      "config": {
        "usesNonExemptEncryption": false
      },
      "associatedDomains": ["applinks:discrapp.com"],
      "infoPlist": {
        "LSApplicationQueriesSchemes": ["venmo"]
      },
      "splash": {
        "image": "./assets/images/splash-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#3b1877"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#3b1877"
      },
      "package": "com.discr.app",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "discrapp.com",
              "pathPrefix": "/d/"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "@sentry/react-native/expo"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "updates": {
      "url": "https://u.expo.dev/da5579bb-8ed9-4c5d-8090-299146e0fc6a"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "da5579bb-8ed9-4c5d-8090-299146e0fc6a"
      }
    },
    "owner": "benniemosher-aceback"
  }
}
